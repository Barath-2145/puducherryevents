        String.prototype.removeValidInvalidCharacters = function (validinvalidChars) {
            var res = "";
            for (var i = 0; i < this.length; i++) {
                var c = this.charAt(i);
                if (validinvalidChars.indexOf(c) < 0) {
                    if (c.search(/[^a-zA-Z]/) != -1) {
                        res += c;
                    }
                }
            }
            return res;
        }

        String.prototype.removeValidInvalidCharacters = function (validinvalidChars) {
            var res = "";
            for (var i = 0; i < this.length; i++) {
                var c = this.charAt(i);
                if (validinvalidChars.indexOf(c) < 0) {
                    if (c.search(/[^a-zA-Z]/) != -1) {
                        res += c;
                    }
                }
            }
            return res;
        };

        String.prototype.removeInvalidCharacters = function (invalidChars) {
            var s = "";
            for (var i = 0; i < this.length; i++) {
                var c = this.charAt(i);
                if (invalidChars.indexOf(c) < 0)
                    s += c;
            }
            return s;
        };

        String.prototype.allowValidCharacters = function (validChars) {
            var s = "";
            for (var i = 0; i < this.length; i++) {
                var c = this.charAt(i);
                if (validChars.indexOf(c) >= 0)
                    s += c;
            }
            return s;
        };

        String.prototype.capitalize = function () {
            return this.replace(/(^|\s)([a-z])/g, function (m, p1, p2) {
                return p1 + p2.toUpperCase();
            });
        };

        String.prototype.capitalizeFirstLetter = function () {
            return this.charAt(0).toUpperCase() + this.slice(1);
        };

        function onLogout() {

            document.cookie.split(';').forEach(function (c) {
                console.log(c);

                document.cookie = c.trim().split('=')[0] + '=;' + 'expires=Thu, 01 Jan 1970 00:00:00 UTC;';
            });

            clearLocalData();

            setTimeout(function () {
                window.location = "/Home/Logout";
            }, 0);

            return false;
        }

        var userID = window.localStorage.getItem("UserID");
